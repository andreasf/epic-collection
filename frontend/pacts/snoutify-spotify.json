{
  "consumer": {
    "name": "snoutify"
  },
  "provider": {
    "name": "spotify"
  },
  "interactions": [
    {
      "description": "OPTIONS /v1/me/tracks",
      "providerState": "with 3 tracks",
      "request": {
        "method": "OPTIONS",
        "path": "/v1/me/tracks",
        "headers": {
          "Origin": "http://localhost",
          "Access-Control-Request-Method": "GET",
          "Access-Control-Request-Headers": "authorization"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Access-Control-Allow-Origin": "http://localhost",
          "Access-Control-Allow-Credentials": "true",
          "Access-Control-Allow-Headers": "authorization"
        }
      }
    },
    {
      "description": "GET /v1/me/tracks (page 1)",
      "providerState": "with 3 tracks",
      "request": {
        "method": "GET",
        "path": "/v1/me/tracks",
        "query": "offset=0&limit=2",
        "headers": {
          "Authorization": "Bearer real-access-token"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "total": 3
        }
      }
    },
    {
      "description": "OPTIONS /v1/me",
      "providerState": "with test user",
      "request": {
        "method": "OPTIONS",
        "path": "/v1/me",
        "headers": {
          "Origin": "http://localhost",
          "Access-Control-Request-Method": "GET",
          "Access-Control-Request-Headers": "authorization"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Access-Control-Allow-Origin": "http://localhost",
          "Access-Control-Allow-Credentials": "true",
          "Access-Control-Allow-Headers": "authorization"
        }
      }
    },
    {
      "description": "GET /v1/me",
      "providerState": "with test user",
      "request": {
        "method": "GET",
        "path": "/v1/me",
        "headers": {
          "Authorization": "Bearer real-access-token"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
